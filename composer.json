{
    "name": "stil/curl-easy",
    "description": "cURL wrapper for PHP. Supports parallel and non-blocking requests. For high speed crawling, see stil/curl-robot.",
    "license": "MIT",
    "require": {
        "symfony/event-dispatcher": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "cURL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "cURL\\Tests\\": "tests/"
        }
    }
}
