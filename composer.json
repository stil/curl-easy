{
  "name": "exteon/curl-easy",
  "version": "2.0.0",
  "description": "cURL wrapper for PHP. Supports parallel and non-blocking requests. For high speed crawling, see stil/curl-robot.",
  "license": "MIT",
  "require": {
    "symfony/event-dispatcher": "^5.0 || ^6.0",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "cURL\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "cURL\\Tests\\": "tests/"
    }
  }
}
