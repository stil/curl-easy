{
    "name": "stil/curl-easy",
    "description": "cURL wrapper for PHP. Supports parallel and non-blocking requests. For high speed crawling, see stil/curl-robot.",
    "license": "MIT",
    "require": {
        "symfony/event-dispatcher": "^4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "cURL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "cURL\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}
